forward Fmadmins(rej, plname[], plmoney, &twenlim, &restlim);
public Fmadmins(rej, plname[], plmoney, &twenlim, &restlim)
{
	new file, f1[256], f2[256];
	format(f1, sizeof(f1), "madmins/players/%s.ini", plname);
	if(rej == 0)
	{
		if(fexist(f1))
		{
			new data1;
			file = ini_openFile(f1);
			if(file >= 0)
			{
				ini_getInteger(file, "DataNm", data1);
				ini_closeFile(file);
			}
			format(f2, sizeof(f2), "madmins/data/%d.ini", data1);
			if(fexist(f2))
			{
				fremove(f2);
			}
			fremove(f1);
		}
		return 1;
	}
	gettime(timecor[0], timecor[1]);
	getdate(timecor[2], timecor[3], timecor[4]);
	TimCor();//коррекция времени
	DatCor();//коррекция даты
	new para1;
	para1 = ((timecor[2] - 2000) * 366) + (timecor[3] * 31) + timecor[4];
	if(!fexist(f1))
	{
		new para3;
		file = ini_createFile(f1);
		if(file >= 0)
		{
			ini_setInteger(file, "DataDay", para1);
			ini_setInteger(file, "TwenLim", 0);
			ini_setInteger(file, "RestLim", 0);
			new para2 = 0;
			para3 = 0;
			while(para2 < 120)
			{
				format(f2, sizeof(f2), "madmins/data/%d.ini", para2);
				if(!fexist(f2))
				{
					para3 = 1;
					break;
				}
				para2++;
			}
			if(para3 == 0)
			{
				print("Ошибка !!! Превышен лимит файлов в папке scriptfiles/madmins/data/  !!!");
				SendAdminMessage(COLOR_RED, " Ошибка !!! Превышен лимит файлов в папке scriptfiles/madmins/data/  !!!");
				ini_setInteger(file, "DataNm", -1);
			}
			else
			{
				ini_setInteger(file, "DataNm", para2);
			}
			ini_closeFile(file);
		}
		if(para3 == 1)
		{
			if(!fexist(f2))
			{
				file = ini_createFile(f2);
			}
			else
			{
				file = ini_openFile(f2);
			}
			if(file >= 0)
			{
				ini_setString(file, "DataName", plname);
				ini_setInteger(file, "TwenLim", 0);
				ini_setInteger(file, "RestLim", 0);
				ini_closeFile(file);
			}
		}
	}
	if(rej == 1)
	{
		new para2;
		file = ini_openFile(f1);
		if(file >= 0)
		{
			restlim = twenlim;
			ini_setInteger(file, "DataDay", para1);
			ini_setInteger(file, "TwenLim", twenlim);
			ini_setInteger(file, "RestLim", restlim);
			ini_getInteger(file, "DataNm", para2);
			ini_closeFile(file);
		}
		if(para2 != -1)
		{
			format(f2, sizeof(f2), "madmins/data/%d.ini", para2);
			if(!fexist(f2))
			{
				file = ini_createFile(f2);
			}
			else
			{
				file = ini_openFile(f2);
			}
			if(file >= 0)
			{
				ini_setString(file, "DataName", plname);
				ini_setInteger(file, "TwenLim", twenlim);
				ini_setInteger(file, "RestLim", restlim);
				ini_closeFile(file);
			}
		}
		return 1;
	}
	new data1, data2, data3, data4;
	data3 = 0;
	file = ini_openFile(f1);
	if(file >= 0)
	{
		ini_getInteger(file, "DataDay", data1);
		ini_getInteger(file, "TwenLim", twenlim);
		ini_getInteger(file, "RestLim", restlim);
		ini_getInteger(file, "DataNm", data2);
		if(data1 != para1)
		{
			data3 = 1;
			data1 = para1;
			restlim = twenlim;
			ini_setInteger(file, "DataDay", data1);
			ini_setInteger(file, "RestLim", restlim);
		}
		if(twenlim == 0)
		{
			data4 = 1;
		}
		else
		{
			para1 = restlim - plmoney;
			if(para1 < 0)
			{
				data4 = 0;
			}
			else
			{
				data3 = 1;
				data4 = 1;
				restlim = para1;
				ini_setInteger(file, "RestLim", restlim);
			}
		}
		ini_closeFile(file);
	}
	if(data2 != -1 && data3 == 1)
	{
		format(f2, sizeof(f2), "madmins/data/%d.ini", data2);
		if(!fexist(f2))
		{
			file = ini_createFile(f2);
		}
		else
		{
			file = ini_openFile(f2);
		}
		if(file >= 0)
		{
			ini_setString(file, "DataName", plname);
			ini_setInteger(file, "TwenLim", twenlim);
			ini_setInteger(file, "RestLim", restlim);
			ini_closeFile(file);
		}
	}
	return data4;
}

forward SpectateStop(playerid);
public SpectateStop(playerid)//отключение наблюдения (если за игроком наблюдает VIP)
{
	for(new i; i < MAX_PLAYERS; i++)
	{
		if(IsPlayerConnected(i))//дальнейшее выполняем если игрок в коннекте
		{
			if(admper1[i] == playerid && PlayerInfo[i][pAdmin] == 0 &&//если за игроком наблюдает VIP,
			GetPVarInt(i, "TotPr") == 0)//и у полицейского нет защитного тотема, то:
			{
				TogglePlayerSpectating(i, 0);//запретить наблюдение для полицейского
				admper6[i] = 0;//обнуляем отметку о переключении наблюдения
			}
		}
	}
	return 1;
}

