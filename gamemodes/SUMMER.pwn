public OnPlayerDeath(playerid, killerid, reason)
{
	if(functioncon[playerid] < 0)//если счётчик секунд меньше нуля, то:
	{
		return 1;//завершаем функцию
	}
	if(functioncon[playerid] > 5)//если игрок вызвал функции более 5-и раз за 1 секунду, то:
	{
		functioncon[playerid] = -5;//устанавливаем счётчик 5-и секунд
	}
	SetPlayerArmour(killerid, 100);
	SetPlayerHealth(killerid, 100);
	new string[144];
    PlayerTotalKills[killerid]++;
    PlayerTotalKills[playerid] = 0;
    if(PlayerTotalKills[killerid] == 1)
	{
    format(string, sizeof(string), "~g~1/25 Kill");
    GameTextForPlayer(killerid,string,3000, 1);
    }
    if(PlayerTotalKills[killerid] == 2)
    {
    format(string, sizeof(string), "~g~2/25 Kill");
    GameTextForPlayer(killerid,string,3000, 1);
    }
    if(PlayerTotalKills[killerid] == 3)
	{
    format(string, sizeof(string), "~g~3/25 Kill");
    GameTextForPlayer(killerid,string,3000, 1);
    }
    if(PlayerTotalKills[killerid] == 4)
	{
    format(string, sizeof(string), "~g~4/25 Kill");
    GameTextForPlayer(killerid,string,3000, 1);
    }
    if(PlayerTotalKills[killerid] == 5)
	{
    format(string, sizeof(string), "~g~5/25 Kill");
    GivePlayerCash(playerid,15000);
    SendClientMessage(killerid, COLOR_GREEN1, "+ 15.000 $. Вы совершили серию убийств [5/25]!");
    GameTextForPlayer(killerid,string,3000, 1);
    }
    if(PlayerTotalKills[killerid] == 6)
	{
    format(string, sizeof(string), "~g~6/25 Kill");
    GameTextForPlayer(killerid,string,3000, 1);
    }
    if(PlayerTotalKills[killerid] == 7)
	{
    format(string, sizeof(string), "~g~7/25 Kill");
    GameTextForPlayer(killerid,string,3000, 1);
    }
    if(PlayerTotalKills[killerid] == 8)
	{
    format(string, sizeof(string), "~g~8/25 Kill");
    GameTextForPlayer(killerid,string,3000, 1);
    }
    if(PlayerTotalKills[killerid] == 9)
	{
    format(string, sizeof(string), "~g~9/25 Kill");
