public OnPlayerClickMap(playerid, Float:fX, Float:fY, Float:fZ)
{
        if(PlayerInfo[playerid][pAdmin] >=1)
        {
                new vehicleid = GetPlayerVehicleID(playerid); // Узнаем ID машины в которой сидит игрок
                if(vehicleid > 0 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER) // Если игрок в транспорте и он водитель
                {
                        SetVehiclePos(vehicleid, fX, fY, fZ); // Телепорт транспорта
                }
                else // Если игрок НЕ находится в транспорте
                {
                        SetPlayerPos(playerid, fX, fY, fZ); // Телепорт игрока
                }
                SendClientMessage(playerid, -1, "{bebebe}[<AC>] Вы были успешно телепортированы."); // Сообщение о успешном телепорте
        }
        return 1;
}
stock ProxDetector(Float:radi, playerid, str[],col1,col2,col3,col4,col5)
{
	if(IsPlayerConnected(playerid))
	{
		new Float:posx, Float:posy, Float:posz;
		new Float:oldposx, Float:oldposy, Float:oldposz;
		new Float:tempposx, Float:tempposy, Float:tempposz;
		GetPlayerPos(playerid, oldposx, oldposy, oldposz);
		//radi = 2.0; //Trigger Radius
		for(new i = 0; i < MAX_PLAYERS; i++)
		{
			if(IsPlayerConnected(i))
			{

				GetPlayerPos(i, posx, posy, posz);
				tempposx = (oldposx -posx);
				tempposy = (oldposy -posy);
				tempposz = (oldposz -posz);
				//printf("DEBUG: X:%f Y:%f Z:%f",posx,posy,posz);
				if (((tempposx < radi/16) && (tempposx > -radi/16)) && ((tempposy < radi/16) && (tempposy > -radi/16)) && ((tempposz < radi/16) && (tempposz > -radi/16)))
				{
					SendClientMessage(i, col1, str);
				}
				else if (((tempposx < radi/8) && (tempposx > -radi/8)) && ((tempposy < radi/8) && (tempposy > -radi/8)) && ((tempposz < radi/8) && (tempposz > -radi/8)))
				{
					SendClientMessage(i, col2, str);
				}
				else if (((tempposx < radi/4) && (tempposx > -radi/4)) && ((tempposy < radi/4) && (tempposy > -radi/4)) && ((tempposz < radi/4) && (tempposz > -radi/4)))
				{
					SendClientMessage(i, col3, str);
				}
				else if (((tempposx < radi/2) && (tempposx > -radi/2)) && ((tempposy < radi/2) && (tempposy > -radi/2)) && ((tempposz < radi/2) && (tempposz > -radi/2)))
				{
					SendClientMessage(i, col4, str);
				}
				else if (((tempposx < radi) && (tempposx > -radi)) && ((tempposy < radi) && (tempposy > -radi)) && ((tempposz < radi) && (tempposz > -radi)))
				{
					SendClientMessage(i, col5, str);
				}
			}
		}
	}
	return 1;
}
Smena_Nicka(playerid, newname[])
{
	new string[128];
	format(string,sizeof(string),"players/%s.ini", newname);
//        	format(string,sizeof(string),"%s.ini",akk);
	if(fexist(string))
	{
		SendClientMessage(playerid, COLOR_RED, "Такой ник занят!");
		return 1;
	}
	for(new i=0;i<MAX_PLAYERS;i++)//проверка аккаунта на On-Line
	{
		if(IsPlayerConnected(i))
 		{
			if(strcmp(newname, RealName[i], false) == 0)
			{
				format(string,sizeof(string)," Нельзя, аккаунт игрока [%s] On-Line !", newname);
				SendClientMessage(playerid, COLOR_RED, string);
				return 1;
			}
		}
	}
	new string_r[56];

	new name[MAX_PLAYER_NAME];
 	GetPlayerName(playerid, name, sizeof(name));
	format(string_r, sizeof string_r, "players/%s.ini", name);
	fremove(string_r);
	SetPlayerName(playerid, newname);
	format(string, sizeof(string), "{bebebe}[<AC>] {FCFF5C}Игрок %s {bebebe}теперь известен как {FCFF5C}%s",RealName[playerid], newname);
	SendAdminMessage(COLOR_LIGHTRED, string);
//    SendClientMessageToAll(COLOR_LIGHTRED,string);

	new pname[MAX_PLAYER_NAME];
	GetPlayerName(playerid,pname,sizeof(pname));
	strdel(RealName[playerid],0,MAX_PLAYER_NAME);//очистить реальный ник игрока
	new aa333[64];//доработка для использования Русских ников
	format(aa333, sizeof(aa333), "%s", pname);//доработка для использования Русских ников
	strcat(RealName[playerid], aa333);//запомнить реальный ник игрока (доработка для использования Русских ников)
	SendClientMessage(playerid, COLOR_LIGHTBLUE, "{FCFF5C}[SERVER]: {FFFFFF}Вы успешно изменили свой игровой НикНейм! Не забудьте изменить Ник в лаунчере SAMP");
	PlayerInfo[playerid][pDonateO] -= 50;
	GameTextForPlayer(playerid, "~g~Successfully", 700, 1);

 	OnPlayerSaveA(playerid);
	return 1;
}
