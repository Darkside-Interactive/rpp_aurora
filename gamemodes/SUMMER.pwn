public DmZone(playerid)
{
	if(dzona[playerid] == 0) return SendClientMessage(playerid, COLOR_RED, "{FCFF5C}[SERVER]: {FFFFFF}Вы не на DeathMatch зоне");
	if(GetPlayerVehicleID(playerid) != 0)//если игрок в транспорте, то:
	{
		SetPVarInt(playerid,"Veh",0);
	 	if(playcar[playerid] != 0)
 		{
   			DestroyVehicle(playcar[playerid]);
			playcar[playerid] = 0;
   		}
	}
	SetPlayerColor(playerid, tPlayerColor[playerid]);
 	tPlayerTeam[playerid] = 0;
	SetPlayerTeam(playerid, -1);
	dzona[playerid] = 0;
	OnPlayerSpawn(playerid);
	ResetPlayerWeapons(playerid);
	SendClientMessage(playerid, COLOR_RED,"{FCFF5C}[SERVER]: {FFFFFF}Вы успешно покинули DM зону");
    return 1;
}

Float:GetPlayerTheoreticAngle(i)
{
    new Float: sin, Float: dis, Float: angle2, Float: x, Float: y, Float: z, Float: tmp3, Float: tmp4, Float: MindAngle;
    GetPlayerPos(i, x, y, z);
    dis = floatsqroot(floatpower(floatabs(floatsub(x, ppos[i][0])), 2) + floatpower(floatabs(floatsub(y, ppos[i][1])), 2));
    if(IsPlayerInAnyVehicle(i)) GetVehicleZAngle(GetPlayerVehicleID(i), angle2); else GetPlayerFacingAngle(i, angle2);
    if(x > ppos[i][0]) tmp3 = x - ppos[i][0];
	else tmp3 = ppos[i][0] - x;
    if(y > ppos[i][1]) tmp4 = y - ppos[i][1];
    else tmp4 = ppos[i][1] - y;
    if(ppos[i][1] > y && ppos[i][0] > x)
    {
        sin = asin(tmp3/dis);
        MindAngle = floatsub(floatsub(floatadd(sin, 90), floatmul(sin, 2)), -90.0);
    }
    if(ppos[i][1] < y && ppos[i][0] > x)
    {
        sin = asin(tmp3/dis);
        MindAngle = floatsub(floatadd(sin, 180), 180.0);
    }
    if(ppos[i][1] < y && ppos[i][0] < x)
    {
        sin = acos(tmp4/dis);
        MindAngle = floatsub(floatadd(sin, 360), floatmul(sin, 2));
    }
    if(ppos[i][1] > y && ppos[i][0] < x)
    {
        sin = asin(tmp3/dis);
        MindAngle = floatadd(sin, 180);
    }
    if(MindAngle == 0.0) return angle2;
    else return MindAngle;
}

Float:ReturnPlayerAngle(playerid)
{
    new Float: Ang;
    if(IsPlayerInAnyVehicle(playerid)) GetVehicleZAngle(GetPlayerVehicleID(playerid), Ang); else GetPlayerFacingAngle(playerid, Ang);
    return Ang;
}

public DriftResult(playerid)
{
    SetPVarInt(playerid, "PlMon", GetPVarInt(playerid, "PlMon") + DriftScore[playerid] * 100);
    StopDriftCount(playerid);
    for(new b = 0; b < 3; b++) TextDrawHideForPlayer(playerid, DCTD[playerid][b]);
    return 1;
}

public UpdateAnimForAll()
{
    if(DriftVal[0] <= 0.380000) plus = true;
    if(DriftVal[0] > 0.470000) plus = false;
    if(plus == true)
    {
	    DriftVal[0]+=((0.470000-0.380000)/6);
	    DriftVal[1]+=((1.800000-1.500000)/6);
    }
    else
    {
	    DriftVal[0]-=((0.470000-0.380000)/6);
	    DriftVal[1]-=((1.800000-1.500000)/6);
    }
    foreach(g)
    {
    	if(DriftPoints[g] > 0)
	    {
		    TextDrawLetterSize(DCTD[g][0], DriftVal[0], DriftVal[1]);
		    TextDrawShowForPlayer(g, DCTD[g][0]);
	    }
	}
	KillTimer(ServerTimer[1]);
	ServerTimer[1] = SetTimer("UpdateAnimForAll", 50, false);
}

public DriftCounter()
{
    foreach(g)
    {
        new Float: packetloss = NetStats_PacketLossPercent(g);
        if(packetloss >= 2.5) SetPVarInt(g, "FINISHDRIFT", gettime() + 2);
        new Float: Angle[2], Float: coords[3];
        new vid = GetPlayerVehicleID(g);
        Angle[0] = ReturnPlayerAngle(g);
        Angle[1] = GetPlayerTheoreticAngle(g);
        new dtpoint = DriftScore[g];
        if(GetPlayerState(g) == PLAYER_STATE_DRIVER && GetPVarInt(g,"FINISHDRIFT") < gettime() && !IgnoreVeh(vid) && floatabs(floatsub(Angle[0], Angle[1])) > 10.0 && floatabs(floatsub(Angle[0], Angle[1])) < 90.0 && GetPlayerSpeed(g) > 30.0 && GetPlayerSpeed(g) < 300.0)
        {
            if(DriftTimer[g] > 0) KillTimer(DriftTimer[g]);
            DriftTimer[g] = 0;
            if(DriftScore[g] == 0 && DriftPoints[g] > 0) StopDriftCount(g);
            DriftTimer[g] = SetTimerEx("DriftResult", 3000, false, "d", g);
            DriftScore[g]++;
            DriftPoints[g] += floatround(10 * floatabs(floatsub(Angle[0], Angle[1])));
        }
        if(DriftScore[g] > 0 && dtpoint != DriftScore[g])
        {
	        TextDrawSetString(DCTD[g][1], "~w~DRIFTING");
	        TextDrawShowForPlayer(g, DCTD[g][1]);
	        new s[15];
	        format(s, sizeof(s), "~y~%d", DriftPoints[g]);
	        TextDrawSetString(DCTD[g][0], s);
	        TextDrawShowForPlayer(g, DCTD[g][0]);
	        if(DriftPoints[g] >= 1000)
        {
		new DriftRate[20];
		if(DriftPoints[g] >= 1000 && DriftPoints[g] < 20000) DriftRate = "~b~Good Start";
		else if(DriftPoints[g] >= 20000 && DriftPoints[g] < 40000) DriftRate = "~g~Unstopable";
		else if(DriftPoints[g] >= 40000 && DriftPoints[g] < 80000) DriftRate = "~g~Mega Drift";
		else if(DriftPoints[g] >= 80000 && DriftPoints[g] < 120000) DriftRate = "~y~GodLike Drift";
		else if(DriftPoints[g] >= 120000 && DriftPoints[g] < 200000) DriftRate = "~y~Monster Drift";
		else if(DriftPoints[g] >= 200000 && DriftPoints[g] < 300000) DriftRate = "~r~Unbelievable";
		else if(DriftPoints[g] >= 300000) DriftRate = "~r~Drift Legend";
        TextDrawSetString(DCTD[g][2], DriftRate), TextDrawShowForPlayer(g, DCTD[g][2]);
        }
		}
        if(IsPlayerInAnyVehicle(g)) GetVehiclePos(vid, coords[0], coords[1], coords[2]); else GetPlayerPos(g, coords[0], coords[1], coords[2]);
        ppos[g][0] = coords[0];
        ppos[g][1] = coords[1];
        ppos[g][2] = coords[2];
    }
	return 1;
}

public OnVehicleDamageStatusUpdate(vehicleid, playerid)
{
    if(DriftScore[playerid] > 0)
    {
		new s[15];
		format(s, sizeof(s), "~r~CRASHED");
		TextDrawSetString(DCTD[playerid][0], s);
		TextDrawShowForPlayer(playerid, DCTD[playerid][0]);
		TextDrawSetString(DCTD[playerid][1], "~w~DRIFTING");
		TextDrawShowForPlayer(playerid, DCTD[playerid][1]);
		TextDrawHideForPlayer(playerid, DCTD[playerid][2]);
		SetPVarInt(playerid, "FINISHDRIFT" ,gettime() + 2);
		DriftScore[playerid]  = 0;
	}
	return 1;
}

