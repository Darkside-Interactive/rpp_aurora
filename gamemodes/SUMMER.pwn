				SendClientMessageToAll(0xFF0000FF, string);
				SetTimerEx("PlayKick", 300, 0, "i", g);
			}
//-------------------- антикрашер игроков - 6 (конец) --------------------------
		}
	}
	nick5col++;
	if (nick5col == 18)
