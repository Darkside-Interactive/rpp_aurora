//-------------------- антикрашер игроков - 3 (конец) --------------------------
	if(newstate >= 1 && newstate <= 3 && oldstate == 8)//повторное включение наблюдения за игроком
	{//после респавна игрока
		for(new i = 0; i < MAX_PLAYERS; i++)
